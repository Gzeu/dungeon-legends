{
  "name": "dungeon-legends-rpg",
  "version": "2.0.0",
  "private": true,
  "description": "Medieval fantasy RPG with card-based combat and character progression",
  "keywords": [
    "rpg",
    "card-game",
    "fantasy",
    "multiplayer",
    "next.js",
    "vercel"
  ],
  "author": "Perplexity AI",
  "license": "MIT",
  "homepage": "https://dungeon-legends-rpg.vercel.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/Gzeu/dungeon-legends.git",
    "directory": "feature/vercel-mvp-rpg"
  },
  "scripts": {
    "dev": "next dev",
    "ws:server": "node server.js",
    "ws:client": "npm run dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "db:connect": "node -e \"require('./lib/db.ts').connectDB().then(() => console.log('MongoDB connected')).catch(console.error)\"",
    "db:test": "tsx scripts/test-db.ts",
    "db:seed": "tsx scripts/seed.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "analyze": "ANALYZE=true npm run build",
    "assets:check": "node scripts/fetch-all-assets.mjs --check",
    "assets:fetch": "node scripts/fetch-all-assets.mjs",
    "assets:icons": "node scripts/resize-icons-improved.mjs",
    "assets:all": "npm run assets:fetch && npm run assets:icons",
    "assets:validate": "npm run assets:check"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@tailwindcss/typography": "^0.5.10",
    "@types/howler": "^2.2.11",
    "@types/jsonwebtoken": "^9.0.10",
    "@vercel/analytics": "^1.1.1",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.16.16",
    "howler": "^2.2.4",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.294.0",
    "mongoose": "^8.19.1",
    "next": "^14.0.3",
    "next-auth": "^4.24.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.8.0",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.7.4",
    "tailwindcss": "^3.3.6",
    "use-sound": "^4.0.1",
    "ws": "^8.14.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-config-next": "^14.0.3",
    "jest": "^29.7.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "sharp": "^0.33.0",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.17.0",
    "npm": ">=9.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
